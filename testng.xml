<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="methods" name="Test Suite" verbose="10">
  <listeners>
    <listener class-name="com.autumn.reporting.listeners.RetryListener"/>
  </listeners>
  <test thread-count="5" parallel="methods" name="Test Suite" verbose="10">
    <groups>
      <run>
        <include name="sanity"/>
      </run>
    </groups>
    <packages>
      <package name="systemTesting.*"/>
    </packages>
  </test> <!-- Test Suite -->
</suite> <!-- Test Suite -->
